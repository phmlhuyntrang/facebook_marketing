<odoo>
    <data>
        <record id="group_manager_account_user" model="res.groups"> 
            <field name="name">Manager Account User</field>
            <field name ="category_id" ref="base.module_category_tutorial"/>
            <field name ="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        
        <record id="group_manager_account_manager" model="res.groups"> 
            <field name="name">Manager Account Manager</field>
            <field name ="category_id" ref="base.module_category_tutorial"/>
            <field name ="implied_ids" eval="[(4, ref('group_manager_account_user'))]"/>
            <field name ="users" eval="[(4, ref('base.user_root')),(4, ref('base.user_admin'))]"/>
        </record>
    </data>
</odoo>